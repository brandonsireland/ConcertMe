<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Page Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="main.css">
</head>

<body>
    <div class="navigation">
        <div class="navigation__container">
            <div class="navigation__logo">
                <span>Concert Me</span>
            </div>
            <div class="navigation__person">
                <span>
                    Hello <span>Brandon</span> !
                </span>
            </div>
            <button class="navigation__settings">
                <i class="fal fa-cog fa-3x"></i>
            </button>

        </div>
    </div>
    <div class="main">
        <div class="main__container">
            <div class="filter">
                <div class="filter__container">
                    <div class="filter__range__container">
                        <span class="filter__title">Date</span>
                        <div class="filter__range"></div>
                        <span class="filter__date--start"></span> - <span class="filter__date--end"></span>
                    </div>
                    <div class="filter__range__container">
                        <span class="filter__title">Location</span>
                        <div class="filter__range--location"></div>
                        <span class="filter__start--location"></span> - <span class="filter__ender--location"></span>
                    </div>
                    <div class="filter__range__container">
                        <span class="filter__title">Price</span>
                        <div class="filter__range--price"></div>
                        <span class="filter__start--price"></span> - <span class="filter__end--price"></span>
                    </div>
                </div>
            </div>
            <div class="sidebar">
                <div class="sidebar__container">
                    <!-- artist -->
                    {{#each data}}
                    <div class="sidebar__artist">
                        <div class="sidebar__photo">
                            <img src="{{ imageUrl }}" alt="">
                        </div>
                        <div class="sidebar__info">
                            <span>{{ artist }}</span>
                        </div>
                        {{!-- <div class="sidebar__tickets">
                            <button>
                                <span>Buy Tickets</span>
                            </button>
                        </div> --}}
                    </div>
                    {{/each}}
                    <!-- end artist -->
                </div>
            </div>
            <div class="map">
                <div class="map__container">

                </div>
            </div>
        </div>
    </div>
    <script src="bundle.js"></script>
</body>

</html>
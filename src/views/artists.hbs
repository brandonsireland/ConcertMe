<body>
    <div class="artists">
        <!-- Grid -->
        <div class="artists__container">
            <!-- Grid item -->
        {{#each data}}
            <div class="artist">
                <div class="artist__container">
                    <div class="artist__image">
                        <!-- Artist Image -->
                        <img src="{{ imageUrl }}" alt="">
                    </div>
                    <div class="artist__info">
                        <div class="artist__name">
                            <!-- Artist Name -->
                            <span>{{ artist }}</span>
                        </div>
                        <div class="artist__genre">
                            <span>Genres:</span>
                            <ul>
                            {{#each genres}}
                                <li>
                                    <span>{{this}}</span>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                        <div class="artist__links">
                            <button class="artist__button">
                                <a href="">
                                    <span>Link to Artist</span>
                                </a>
                            </button>
                            <button class="artist__button">
                                <a href="">
                                    <span>buy tickets</span>
                                </a>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- end Grid item -->
            {{/each}}
        </div>
    </div>

